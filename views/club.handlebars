	<div>{{searchvalue}}</div>

<!-- CLUB DIV -->

	{{#each friends}}

	<div class="striped container1 img-rounded" id="{{link}}" name="{{name}}" description="{{description}}">
	
	<div class="col-xs-12"><center><img src="{{imageURL}}" style="width:248px;" class="img-responsive img-rounded"/></center>


	</div>


	<div class="descripdiv2" id="{{name}}">
	 <center>
		<div class="clubnamestext">
			<h4 class="col-xs-12 namestripe">{{name}}

				<div id="join{{link}}star" style="position:absolute;font-size:9px;margin-top:-7%;margin-left:83%;display:none;"><i style="color:yellow;" class="fa fa-star fa-2x"></i></div>
				<div id="join{{link}}starnot" style="position:absolute;font-size:9px;margin-top:-7%;margin-left:83%;"><i style="color:yellow;" class="fa fa-star-o fa-2x"></i></div>
			</h4>
		</div>

	<p class="descriptiontext">{{description}}</p>

	</center>
	</div>

	<div class="btn-group" style="margin-top:-12px;" id="{{name}}">
  <div class="btn btn-default" style="font-size:10px" href="#"><i class="fa fa-tags"></i></div>
  {{#if tag1}}<div class="btn btn-default" style="font-size:10px" href="#">{{tag1}}</div>{{/if}}
    {{#if tag2}}<div class="btn btn-default" style="font-size:10px" href="#">{{tag2}}</div>{{/if}}
        {{#if tag3}}<div class="btn btn-default" style="font-size:10px" href="#">{{tag3}}</div>{{/if}}
